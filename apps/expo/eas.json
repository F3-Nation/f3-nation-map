{
  "cli": {
    "version": ">= 4.1.2"
  },
  "build": {
    "base": {
      "node": "18.16.1"
    },
    "dev": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://dev.template.mountaindev.com",
        "APP_ICON": "./assets/icon-test.png",
        "APP_NAME": "T3 Template (Dev)"
      }
    },
    "staging": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://staging.template.mountaindev.com",
        "APP_ICON": "./assets/icon-test.png",
        "APP_NAME": "T3 Template (Staging)"
      }
    },
    "prod": {
      "extends": "base",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://template.mountaindev.com"
      }
    }
  },
  "submit": {
    "dev": {
      "android": {
        "track": "internal",
        "releaseStatus": "completed"
      },
      "ios": {
        "ascAppId": "1640693113"
      }
    },
    "staging": {
      "extends": "dev"
    },
    "prod": {
      "extends": "staging"
    }
  }
}
