{
  "name": "@acme/nextjs",
  "version": "2.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "rm -rf .turbo node_modules .next",
    "dev": "TZ=UTC pnpm with-env next dev",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "test": "pnpm with-env vitest --run",
    "test:watch": "pnpm with-env vitest -w",
    "test:e2e": "pnpm with-env playwright test",
    "test:e2e:staging": "pnpm with-env playwright test --config=playwright.staging.config.ts",
    "test:e2e:debug": "pnpm with-env playwright test --debug",
    "test:e2e:ui": "pnpm with-env playwright test --ui",
    "test:e2e:report": "pnpm with-env playwright show-report",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@acme/api": "workspace:^0.1.0",
    "@acme/auth": "workspace:^0.1.0",
    "@acme/db": "workspace:^0.1.0",
    "@acme/env": "workspace:^0.1.0",
    "@acme/shared": "workspace:^0.1.0",
    "@acme/tailwind-config": "workspace:^0.1.0",
    "@acme/ui": "workspace:^0.1.0",
    "@acme/validators": "workspace:^0.1.0",
    "@formkit/auto-animate": "^0.8.1",
    "@radix-ui/react-dialog": "^1.0.4",
    "@react-hook/window-size": "^3.1.1",
    "@sentry/nextjs": "^9.15.0",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tanstack/react-query": "^5.59.15",
    "@tanstack/react-query-devtools": "^5.17.7",
    "@trpc/client": "^11.0.0-rc.477",
    "@trpc/react-query": "^11.0.0-rc.477",
    "@trpc/server": "^11.0.0-rc.477",
    "@vis.gl/react-google-maps": "^1.5.2",
    "axios": "^1.7.4",
    "dayjs": "^1.11.10",
    "dompurify": "^3.1.7",
    "dotenv-cli": "^7.3.0",
    "geist": "^1.2.2",
    "handlebars": "^4.7.8",
    "handlebars-loader": "^1.7.3",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "next": "^15.3.1",
    "next-plausible": "^3.12.0",
    "react": "18.3.1",
    "react-div-100vh": "^0.7.0",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.50.1",
    "react-lottie": "^1.2.4",
    "react-qr-code": "^2.0.15",
    "react-responsive": "^9.0.2",
    "supercluster": "^8.0.1",
    "superjson": "2.2.1",
    "trpc-openapi": "^1.2.0",
    "uuid": "^11.0.5",
    "vaul": "^0.9.0",
    "zod": "^3.24.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:^0.2.0",
    "@acme/prettier-config": "workspace:^0.1.0",
    "@acme/tsconfig": "workspace:*",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@types/dompurify": "^3.0.5",
    "@types/geojson": "^7946.0.16",
    "@types/google.maps": "^3.58.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.11.13",
    "@types/nodemailer": "^6.4.17",
    "@types/nodemailer-smtp-transport": "^2.7.8",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/supercluster": "^7.1.3",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.56.0",
    "jiti": "^1.21.0",
    "jsdom": "^24.0.0",
    "msw": "^2.2.14",
    "msw-trpc": "2.0.0-beta.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.0",
    "vitest-canvas-mock": "^0.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@acme/eslint-config/base",
      "@acme/eslint-config/nextjs",
      "@acme/eslint-config/react"
    ]
  },
  "prettier": "@acme/prettier-config"
}
